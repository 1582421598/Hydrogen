{
  RelativeLayout;
  layout_width="-1";
  background=backgroundc;
  layout_height="-1";
  {
    LinearLayout;
    layout_width="-1";
    layout_height="-1";
    orientation="vertical";
    {
      LinearLayout;
      layout_width="-1";
      layout_height="-2";
      elevation="0";
      background=backgroundc;
      orientation="vertical";
      {
        LinearLayout;
        layout_width="-1";
        layout_height="56dp";
        gravity="center|left";
        {
          LinearLayout;
          orientation="horizontal";
          layout_height="56dp";
          layout_width="56dp";
          gravity="center";
          onClick=function()关闭页面()end;
          {
            ImageView;
            ColorFilter=primaryc;
            src=图标("arrow_back");
            layout_height="32dp";
            layout_width="32dp";
            padding="4dp";
            id="fh";
          };
        };
        {
          TextView;
          text="设置推荐";
          textColor=primaryc;
          textSize="20sp";
          id="_title";
          SingleLine=true;
          layout_width="-1",
          gravity="center|left";
          paddingLeft="16dp";
          layout_weight="1",
          Typeface=字体("product-Bold");
        };
      };
    };
    {
      ScrollView;
      layout_height="-1";
      layout_width="-1";
      {
        LinearLayout;
        layout_height="-1";
        layout_width="-1";
        orientation="vertical";
        {
          NoScrollListView;
          layout_height="-1";
          layout_width="-1";
          DividerHeight=0;
          id="settings_list";
        };
        {
          EditText;
          id="Editt";
          layout_height="wrap_content";
          layout_width="match_parent";
        };
        {
          EditText;
          id="Edittt";
          layout_height="wrap_content";
          layout_width="match_parent";
        };
        {
          CardView;
          layout_width="-2";
          layout_height="-2";
          radius="4dp";
          layout_gravity="center",
          --background=primaryc;
          CardBackgroundColor=primaryc;
          onClick=function()Editt.Text=kk 提示("恢复成功 请重新编辑") end;
          {
            TextView;
            id="moren",
            layout_width="-1";
            layout_height="-1";
            textSize="14sp";
            paddingRight="12dp";
            paddingLeft="12dp";
            paddingTop="8dp";
            paddingBottom="8dp";
            Text="恢复默认";
            textColor=backgroundc;
            gravity="center";
            Typeface=字体("product-Bold");
          };
        };
        {
          CardView;
          layout_width="-2";
          layout_height="-2";
          layout_marginTop="10dp";
          radius="4dp";
          layout_gravity="center",
          --background=primaryc;
          CardBackgroundColor=primaryc;
          onClick=function()修改页面() end;
          {
            TextView;
            id="xiugai",
            layout_width="-1";
            layout_height="-1";
            textSize="14sp";
            paddingRight="12dp";
            paddingLeft="12dp";
            paddingTop="8dp";
            paddingBottom="8dp";
            Text="立即修改";
            textColor=backgroundc;
            gravity="center";
            Typeface=字体("product-Bold");
          };
        };
      };
    };
  };
};
